<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="icon" format="reference"></attr>
    <attr name="color_select" format="color"></attr>
    <attr name="color_unselect" format="color"></attr>
    <attr name="text" format="string"></attr>
    <attr name="text_size" format="dimension"></attr>

    <declare-styleable name="ChangeColorIconWithText">
        <attr name="icon"></attr>
        <attr name="color_select"></attr>
        <attr name="color_unselect"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
    </declare-styleable>
</resources>